  transparent
  websocket
}

proxy /auth keycloak:8080 {
  transparent
}

proxy /api api:8180 {
  without /api
  transparent
}

proxy /api/files havenapi:3000 {
  transparent
}

log / stdout "{combined}"
errors stdout
